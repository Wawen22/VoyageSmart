{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-debugger": "error",
    "no-alert": "error",
    "prefer-const": "warn",
    "no-var": "error",
    "eqeqeq": "warn",
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-new-func": "error"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
